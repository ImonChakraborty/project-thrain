{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      },
      {
        "src": "src/app.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/app.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
}